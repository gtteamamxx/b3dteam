<Window x:Class="b3dteam_app.View.UserUtilities.RemoveContact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:b3dteam_app.View.UserUtilities"
        mc:Ignorable="d"
        Title="RemoveContact" Height="400" Width="300">
    <Grid>
        <ListView Margin="10"
                  Height="300"
                  VerticalAlignment="Top"
                  Name="listview_Users"/>

        <Button Name="button_Leave"
                Width="100"
                Height="30"
                VerticalAlignment="Bottom"
                Margin="10"
                Content="Leave"
                Click="button_Leave_Click" />
    </Grid>
</Window>
